Coffeemaker.core.capacities.load = function () {

		var repository;
		var payload;
		var path;
		
		repository = arguments[0];
		payload = arguments[1];
		
		Array.prototype.shift.apply(arguments);
		Array.prototype.shift.apply(arguments);
		
		if ( ! payload ) {
				
                //if ( Coffeemaker[repository] && Coffeemaker.core.capacities[repository]) {
                if ( repository !== 'components' && repository !== 'methods' && repository !== 'templates') {
					return Coffeemaker.core.capacities.analyze.engine('core/capacities/'+repository);
                    //return Coffeemaker.core.capacities[repository];
                } else {
					//return Coffeemaker.core.capacities.analyze.engine('core/capacities/'+repository);
					return Coffeemaker.core.capacities.analyze.engine(repository);
                }

        } else if (repository === 'methods' && payload) {
            
			return Coffeemaker.core.capacities.analyze.engine('methods/'+payload);
            
        }else {
		
		if ( repository === 'components' || repository === 'templates' ) {
				var check = Coffeemaker.core.capacities.check;
				var url = repository+'/'+payload;
				
				if ( check.doesExist(Coffeemaker.core.capacities.analyze.engine(url)) 
                        || check.inArray(url,Coffeemaker.core.capacities.load.loaded) ) {

					//return Coffeemaker[repository][payload];
					return Coffeemaker.core.capacities.analyze.engine(repository+'/'+url);
					//var pad = Coffeemaker.core.capacities.analyze.engine(url);
                    //if (check.isFunction(pad)) {
                    //    return new pad;
                    //} else {
                    //    return pad;
                    //}


				
				} else {
				
					//download components and methods
					if (transmission = Coffeemaker.core.capacities.transmission.ajax) {
					
						transmission.GET( url+'.json',"",function(a){
					
								if (check.isJSON(a)) {
								 	//var repo = engine(url);
									//repo[payload] = JSON.parse(a);
									if ( ! Coffeemaker[repository] ) Coffeemaker[repository] = {};
									Coffeemaker[repository][payload] = JSON.parse(a);
								} else {
									Coffeemaker.core.capacities.log.exception(
									'load.js','JSON failed to parse',a);
								}

						});
						
						Coffeemaker.core.capacities.load.loaded.push(url);
				
					}
					
				}
			
			}
		
		}
		
}
		
